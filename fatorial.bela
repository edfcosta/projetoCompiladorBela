main() {
    var float x;
    var float y;
    x = readInput();
    y=fatorial(x);
    printLn y;
}

fun float fatorial(float n) {
    var float resultado;
    var float i;

    resultado = 1;
    i = 1;

    while ((i < n)||(i==n)) then {
        resultado = (resultado * i);
        i = (i + 1);
    };

    return resultado;
}